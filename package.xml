  <name>sfMooToolsFormExtraPlugin</name>
  <channel>plugins.symfony-project.org</channel>
  <summary>Symfony widgets for use with the MooTools javascript framework</summary>
  <description>This plugin contains several widgets (and a validator) for use with the MooTools (http://mootools.net/) javascript framework.</description>
  <lead>
    <name>Jo Carter</name>
    <user>angelsk</user>
    <email>jocarter@holler.co.uk</email>
    <active>yes</active>
  </lead>
  <developer>
    <name>Ben Lancaster</name>
    <user>benlancaster</user>
    <email>benlancaster@holler.co.uk</email>
    <active>yes</active>
  </developer>
  <date>2011-08-11</date>
  <version>
    <release>1.1</release>
    <api>1.0.0</api>
  </version>
  <stability>
    <release>stable</release>
    <api>stable</api>
  </stability>
  <license uri="https://github.com/HollerLondon/sfMooToolsFormExtraPlugin/blob/master/LICENSE">MIT license</license>
  <notes>-</notes>
  <contents>
    <dir name="/">
      <file role="data" name="README.md" />
      <file role="data" name="LICENSE" />
      <dir name="config">
        <file role="data" name="app.yml" />
        <file role="data" name="sfMooToolsFormExtraPluginConfiguration.class.php" />
      </dir>
      <dir name="docs">
        <dir name="images">
          <file role="data" name="datepicker_date.png" />
          <file role="data" name="datepicker_datetime.png" />
          <file role="data" name="datepicker_input.png" />
          <file role="data" name="mooeditable.png" />
          <file role="data" name="string_validator.png" />
        </dir>
      </dir>
      <dir name="lib">
        <dir name="task">
          <file role="data" name="MooToolsPublishAssetsTask.class.php" />
        </dir>
        <dir name="validator">
          <file role="data" name="sfEnhancedValidatorString.class.php" />
        </dir>
        <dir name="vendor">
          <dir name="Datepicker" />
          <dir name="MooEditable" />
        </dir>
        <dir name="widget">
		  <file role="data" name="BaseWidgetMooPicker.class.php" />
          <file role="data" name="sfWidgetFormDateMooPicker.class.php" />
          <file role="data" name="sfWidgetFormDateTimeMooPicker.class.php" />
          <file role="data" name="sfWidgetFormInputDateTimeMooPicker.class.php" />
          <file role="data" name="sfWidgetFormTextareaMooEditable.class.php" />
        </dir>
      </dir>
      <dir name="test">
        <dir name="bootstrap">
          <file role="data" name="unit.php" />
        </dir>
        <dir name="unit">
          <file role="data" name="sfEnhancedValidatorStringTest.php" />
        </dir>
      </dir>
    </dir>
  </contents>
  <dependencies>
    <required>
      <php>
        <min>5.0.0</min>
      </php>
      <pearinstaller>
        <min>1.4.1</min>
      </pearinstaller>
      <package>
        <name>symfony</name>
        <channel>pear.symfony-project.com</channel>
        <min>1.2.0</min>
        <max>2.0.0</max>
        <exclude>2.0.0</exclude>
      </package>
    </required>
  </dependencies>
